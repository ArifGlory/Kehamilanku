        <activity
            android:name=".admin.UbahPetugasActivity"
            android:label="Ubah Petugas" />
        <activity
            android:name=".admin.AddPetugasActivity"
            android:label="Tambah Petugas" />
        <activity
            android:name=".activity.ListPerkembanganJanin"
            android:theme="@style/AppTheme.NoActionBar" />
        <activity
            android:name=".activity.ListPetugasActivity"
            android:theme="@style/AppTheme.NoActionBar" />
        <activity
            android:name=".activity.UbahProfilActivity"
            android:label="Ubah Profil" />
        <activity
            android:name=".activity.ProfilActivity"
            android:theme="@style/AppTheme.NoActionBar" />
        <activity
            android:name=".activity.ListTipsKehamilan"
            android:theme="@style/AppTheme.NoActionBar" />
        <activity
            android:name=".activity.DetailVideoSenam"
            android:label="Video Senam Hamil" />
        <activity
            android:name=".admin.UbahVideoSenam"
            android:label="Ubah Video Senam" />
        <activity
            android:name=".admin.AddVideoSenam"
            android:label="Tambah Video" />
        <activity
            android:name=".activity.ListVideoSenamActivity"
            android:theme="@style/AppTheme.NoActionBar" />
        <activity
            android:name=".admin.UbahTipsKehamilan"
            android:label="Ubah Data" />
        <activity
            android:name=".activity.DetailTipsKehamilan"
            android:label="Tips Kehamilan" />
        <activity
            android:name=".admin.AddTipsActivity"
            android:label="Tambah Tips" />
        <activity
            android:name=".activity.PanelAdminActivity"
            android:label="Panel Admin"
            android:theme="@style/AppTheme.NoActionBar" />
        <activity
            android:name=".activity.HomeActivity"
            android:label="@string/app_name"
            android:theme="@style/AppTheme.NoActionBar">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name=".MainActivity"
            android:theme="@style/AppTheme.NoActionBar" />
    </application>

</manifest>